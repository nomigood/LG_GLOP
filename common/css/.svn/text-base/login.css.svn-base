a:link {color:#555555; text-decoration:none;}
a:visited {color:#555555; text-decoration:none;}
a:hover, a:focus, a:active {color:#000000;  text-decoration:none;}
body{ overflow-x:hidden; }

*{ font-family: "Tahoma", "돋움", Dotum, "굴림", gulim, AppleGothic,  "Sans-serif" ; margin:0; padding:0;}
*{ margin:0; padding:0; }

ul, ol  { list-style-type :none;}

/*layout ------------------------ */
body{ margin:0 auto; padding:0;}
#login_wrapper{position:relative; left:0; top:0; width:755px; height:600px; margin:0 auto; padding-top:0px; background:url(/images/login_bg.png) no-repeat left top; background-position: left 115px; ;    }
.toplogo{ float:left; margin:0; padding:70px 0 0 4px; position:absolute; left:0px ; top:0px ; }
/*layout ------------------------ */

/* 블럭상자 */
.block{ position:absolute; top:50px ; left:60px ; float:left; }

/*로그인상자*/
.login_box{  width:235px ; height:300px;  position:absolute; left:430px;; top:289px ;  font-size:12px;  border:0px solid black; }
.login_box li{  width:260px; height:28px; line-height:28px;  color:#284d7a; font-weight:bold; font-family:Leelawadee, Tahoma ; font-size:13px;  padding-right:5px; display:block; }
.login_box li input{ float:right; height:20px; line-height:20px;padding-left:5px;  border:1px solid #f7f7f7; width:145px; }
.login_box button{ width:95px; height:24px; line-height:24px; margin:0; padding:0 px;  color:#FFFFFF; font-weight:bold; cursor:pointer;  font-size:12px;   font-family: "Tahoma", "돋움", Dotum, "굴림", gulim, AppleGothic,  "Sans-serif"  }
.login_box li .logimg{ padding-top:5px; display:block; }

.login_tb{  width:235px ; height:70px;  position:absolute; left: 30px; top: 295px;  font-size:12px;  border:0px solid black; }
.login_tb tr{  width:260px; height:30px; line-height:30px;  color:#284d7a; font-weight:bold; font-family:Leelawadee, Tahoma ; font-size:13px;  padding-right:5px; display:block; }
.login_tb input{ float:right; height:20px; line-height:20px;padding-left:5px;  /* border:1px solid #f7f7f7; */ border-top:1px solid #898989; border-left:1px solid #898989; border-right:1px solid #c4c4c4; border-bottom:1px solid #c4c4c4; width:145px; }
.login_tb button{ width:95px; height:24px; line-height:24px; margin:0; padding:0 px;  color:#FFFFFF; font-weight:bold; cursor:pointer;  font-size:12px;   font-family: "Tahoma", "돋움", Dotum, "굴림", gulim, AppleGothic,  "Sans-serif"  }
.login_tb .logimg{ padding-top:5px; display:block; }

.login_tb3{  width:235px ; height:70px;  position:absolute; left: 250px; top: 296px;  font-size:12px;  border:0px solid black; }
.login_tb3 tr{  width:260px; height:30px; line-height:30px;  color:#284d7a; font-weight:bold; font-family:Leelawadee, Tahoma ; font-size:13px;  padding-right:5px; display:block; }

.login_tb2{  width:290px ; height:50px;  position:absolute; left: 30px; top:350px ;  font-size:11px;  border:0px solid black; }
.login_tb2 tr{  width:290px; height:28px; line-height:28px;  color:#000; font-family:Tahoma ; padding-right:5px; display:block; letter-spacing: 1px;}
.login_tb2 button{ width:95px; height:24px; line-height:24px; margin:0; padding:0 px;  color:#FFFFFF; font-weight:bold; cursor:pointer;  font-size:12px;   font-family: "Tahoma", "돋움", Dotum, "굴림", gulim, AppleGothic,  "Sans-serif"  }
.login_tb2 .logimg{ padding-top:5px; display:block; }
.login_tb2 a {color:#000; text-decoration:none;}
.login_tb2 a:hover {color:#000; text-decoration:none;}


/*----- foot 관련/로그인화면-스타일시트--*/
.login_log_sfoot {float:left; position:absolute; top:530px ; left:0px; margin:0; vertical-align:bottom; padding-left:0px; width:100%; height:34px; font-size:11px; color:#5d5c5c;  line-height:34px; font-family: "Trebuchet MS", sans-serif;}
.login_log_sfoot span{ padding-left:10px; float:left;  }
.login_log_sfoot span img{vertical-align:-7px;}
.login_foot_stext{ float:left; width:600px; margin:0; padding-left:10px; border:0px solid red; }
.login_foot_stext li{  height:16px; line-height:16px; }

.login_log_foot {float:left; position:absolute; top:600px ; margin:0; vertical-align:bottom; padding-left:180px; width:100%; height:34px; font-size:11px; color:#5d5c5c;  line-height:34px; font-family: "Trebuchet MS", sans-serif;}
.login_log_foot span{ padding-left:10px; float:left;  }
.login_log_foot span img{vertical-align:-7px;}
.login_foot_text{ float:left; width:400px; margin:0; padding-left:10px; border:0px solid red; }
.login_foot_text li{  height:16px; line-height:16px; }

.button_zone{ width:100%; padding-left:180px ;  border:0px solid red; }
/* 로그인버튼*/
.button{ width:85px; height:30px; background:url(/images/btn_login.png) no-repeat scroll top center; display:block; float:left; line-height:24px;  margin-top:15px;  text-align:center; }


.button span{ text-decoration:none; color:#4e76a8; font-weight:bold; }
.button:hover span{ text-decoration:none; color:#4e76a8; font-weight:bold;}


/* select*/
select{ float:right;  border:1px solid #f7f7f7; color:#555555; font-size:12px; }
select option{ float:right;  padding-left: px;  }

